{% extends "default/module.html.jinja2" %}

{# we add a style tag here so that IDEs highlight everything that follows as CSS.
This line won't be included in any rendered output, text in Jinja2 child templates is discarded outside of blocks. #}
<style>

    {% block style_theme %}
        {#
        We can optionally enable dark mode depending on the user's preferences:
        {{ super() }}
        @media (prefers-color-scheme: dark) {
        #}
        :root {
            --pdoc-background: #212529;
        }

        .pdoc {
            --text: #f7f7f7;
            --muted: #9d9d9d;
            --link: #58a6ff;
            --link-hover: #3989ff;
            --code: #333;
            --active: #555;

            --accent: rgb(44, 53, 62);
            --accent2: #85CE36;

            --nav-hover: #85CE36;
            --name: #77C1FF;
            --def: #0cdd0c;
            --annotation: #00c037;
        }
        {# } #}
    {% endblock %}

</style>

{% block nav_title %}
